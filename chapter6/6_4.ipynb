        "%%file 6-4.c\n",
        "#include<stdio.h>\n",
        "\n",
        "/*整数の2乗値を返す関数*/\n",
        "int sqr(int x) \n",
        "{\n",
        "    return x * x;\n",
        "}\n",
        "\n",
        "/*整数の4乗値を返す関数*/\n",
        "int pow4(int x) \n",
        "{\n",
        "    return sqr(x) * sqr(x);\n",
        "}\n",
        "\n",
        "int main() {\n",
        "\n",
        "    printf(\"2 の4乗は % d です。\\n\",  pow4(2));\n",
        "\n",
        "    return 0;\n",
        "}"
