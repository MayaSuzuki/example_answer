        "%%file 8-6.c\n",
        "\n",
        "#include<stdio.h>\n",
        "\n",
        "int factorial(int n)\n",
        "{\n",
        "    int i;\n",
        "    int result = 1;\n",
        " \n",
        "    for(i = 1; i <= n; i++) {\n",
        "        result *= i;\n",
        "    }\n",
        " \n",
        "    return result;\n",
        "}\n",
        "\n",
        "int main() {\n",
        "    \n",
        "    printf(\"5の階乗は%dです。\", factorial(5));\n",
        "\n",
        "    return 0;\n",
        "}"
